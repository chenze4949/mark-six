<h1>Listing drawings</h1>
 
<table>
  <tr>
    <th>Id</th>
    <th>Title</th>
    <th>Type</th>
    <th>Created At</th>
    <th colspan="3"></th>
  </tr>
 
  <% @drawings.each do |drawing| %>
    <tr>
      <td><%= drawing.id %></td>
      <td><%= drawing.title %></td>
      <td><%= drawing.drawing_type %></td>
      <td><%= drawing.created_at %></td>
      <td><%= link_to 'Show', drawing_path(drawing) %></td>
      <td><%= link_to 'Destroy', drawing_path(drawing),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>